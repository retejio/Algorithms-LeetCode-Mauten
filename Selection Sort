def selection_sort(arr):
   
    n = len(arr)  #длина массива
    for i in range(n):  #внешний цикл проходит по каждому элементу массива
        min_index = i  #предполагаем, что текущий элемент минимальный
        for j in range(i + 1, n):  #внутренний цикл ищет минимальный элемент в оставшейся части массива
            if arr[j] < arr[min_index]:  #если найден элемент меньше текущего минимума
                min_index = j  #обновляем индекс минимального элемента
        arr[i], arr[min_index] = arr[min_index], arr[i]  #меняем найденный минимум с текущей позицией
    return arr  #возвращаем отсортированный массив

data = [64, 25, 12, 22, 11] 
print("исходный массив:", data)  
sorted_data = selection_sort(data.copy())  
print("отсортированный массив:", sorted_data) 




Output:
исходный массив: [64, 25, 12, 22, 11]
отсортированный массив: [11, 12, 22, 25, 64]
